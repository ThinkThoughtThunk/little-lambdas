{
  "name": "little-lambdas",
  "author": "Zach Bluedorn",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "parcel --target default",
    "build": "parcel build --target default",
    "test": "jest --watch",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "dependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@jest/types": "^29.5.0",
    "@parcel/core": "^2.8.3",
    "@reduxjs/toolkit": "^1.9.1",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@types/eslint": "^8.21.2",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "eslint": "^8.36.0",
    "eslint-plugin-functional": "^5.0.6",
    "fs-extra": "^11.1.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-watch-typeahead": "^2.2.2",
    "parcel": "latest",
    "size-limit": "^8.1.2",
    "ts-essentials": "^9.3.0",
    "ts-jest": "^29.0.5",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2",
    "use-complex-state": "^1.1.0",
    "webpack": "^5.76.2"
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "packageManager": "yarn@3.4.1",
  "engines": {
    "node": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint --fix src/"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "preventAssignment": true,
  "size-limit": [
    {
      "path": "dist/little-lambdas.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/little-lambdas.esm.js",
      "limit": "10 KB"
    }
  ],
  "targets": {
    "default": {
      "source": "src/index.html"
    }
  }
}
